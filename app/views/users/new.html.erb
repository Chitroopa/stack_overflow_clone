<div class="row">
  <div class="col-md-2"></div>
  <div class="col-md-6">
    <h1> Sign up</h1>
    <%= bootstrap_form_for @user do |f| %>
      <%= f.email_field :email %>
      <%= f.select(:user_type, ['Admin', 'User'])%>
      <%= f.password_field :password %>
      <%= f.password_field :password_confirmation %>
      <%= f.submit "Sign Up", class: 'btn btn-success'%>
    <% end %>
  </div>
</div>
